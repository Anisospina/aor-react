{
  "presets": ["react-app"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/app/components",
          "@screens": "./src/app/screens",
          "@redux": "./src/redux",
          "@assets": "./src/assets",
          "@utils": "./src/utils",
          "@config": "./src/config"
        }
      }
    ]
  ]
}
